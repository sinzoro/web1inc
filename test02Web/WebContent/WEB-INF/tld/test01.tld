<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd" >
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	
	<short-name>test01</short-name>
	<tag>
		<name>test01</name>
		<tag-class>com.tag.Test01</tag-class>
	</tag>
	
	<tag>
		<name>secondTag</name>
		<tag-class>com.tag.SecondTag</tag-class>
		<attribute>
			<name>name</name>
			<required>false</required>
		</attribute>
	</tag>
	
	<tag>
		<name>thirdTag</name>
		<tag-class>com.tag.ThirdTag</tag-class>
	</tag>
	
</taglib>

